{
  "name": "kata-movies-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint ./src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
